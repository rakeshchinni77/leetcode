class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        int cnt = 0;
        int currSum = 0;
        int n = nums.size();
        unordered_map<int,int>mpp;
        mpp[0] = 1;
        for(int i=0;i<n;i++) {
            currSum+=nums[i];
            if(mpp.count(currSum-k)){
                cnt += mpp[currSum-k];
            }
            mpp[currSum]++;
        }
        return cnt;
    }
};
