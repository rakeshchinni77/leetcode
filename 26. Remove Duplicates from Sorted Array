class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
      auto it = unique(nums.begin(),nums.end());//in this unique is used to move the dup,give last distance 
      return distance(nums.begin(),it);// calculate distance
    } 
};
/** brute force 
if (nums.empty()) return 0;

    int i = 0; // Points to last unique element
    for (int j = 1; j < nums.size(); ++j) {
        if (nums[j] != nums[i]) {
            i++;               // Move to next unique position
            nums[i] = nums[j]; // Overwrite duplicate
        }
    }

    return i + 1;
    } 
**/
