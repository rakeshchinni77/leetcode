class Solution {
public:
    string maxSumOfSquares(int num, int sum) {
        if(9*num<sum) return "";
        string res = "";
        for(int i=1;i<=num;i++) {
           int d = min(9,sum);
            res+=to_string(d);
            sum-=d;
        }
        if(sum!=0) return "";
        return res;
    }
};
