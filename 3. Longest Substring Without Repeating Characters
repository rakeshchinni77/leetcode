class Solution {
public:
    int lengthOfLongestSubstring(string s) {
        int start =0;
        int end =0;
        int maxlen = 0;
        unordered_set<char> seen;
        for(int end=0;end<s.size();end++) {
            while(seen.count(s[end])) {
                seen.erase(s[start++]);
            }
            seen.insert(s[end]);
            maxlen = max(maxlen,end-start+1);
        }
        return maxlen;
    }
};
