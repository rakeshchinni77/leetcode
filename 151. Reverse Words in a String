class Solution {
public:
    string reverseWords(string s) {
        int n = s.size();
        string res = "";
        string temp = "";
        for(int i=0;i<n;i++) {
            if(s[i]!=' ') {
                temp+=s[i];
            }
            else if(s[i] == ' ' && temp!="") {
                res = temp+" "+res;
                temp = "";
            }
        }
        if(temp!="") {
            res = temp+" "+res;
            temp="";
        }
        if(!res.empty()) res.pop_back();
        return res;
    }
};
