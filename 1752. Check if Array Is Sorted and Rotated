class Solution {
public:
    void changePosition(vector<int>& nums) {
        rotate(nums.begin(),nums.begin()+1,nums.end());
    }
    bool check(vector<int>& nums) {
        int n = nums.size();
        for(int i=0;i<n;++i) {
            if(is_sorted(nums.begin(),nums.end())) {
                return true;
            }
            else {
                changePosition(nums);
            }
        }
        return false;
    }
};
