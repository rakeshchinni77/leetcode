class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int n=prices.size();
        int max_profit = 0;
        int min_price=INT_MAX;
        for(auto price:prices) {
            if(price<min_price) {
                min_price=price;
            }
            else {
                int profit = price-min_price;
                max_profit = max(profit,max_profit);
            }
        }
        return max_profit;
    }
};
