class Solution {
bool isvowel(char ch) {
    if(ch == 'a' || ch =='e' || ch =='i' ||ch =='o' ||ch =='u' ||ch =='A' ||ch =='E' ||ch =='I' ||ch =='O' ||ch =='U') {
        return true;
    }
    return false;
}
public:
    string reverseVowels(string s) {
        int st=0,end=s.size()-1;
        while(st<end) {
            if(!isvowel(s[st])) {
                st++;
            }
            if(!isvowel(s[end])) {
                end--;
            }
            if(isvowel(s[st]) && isvowel(s[end])) {
                swap(s[st],s[end]);
                st++;
                end--;
            }
            
        }
        return s;
    }
};
