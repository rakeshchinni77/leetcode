class Solution {
public:
    int gcdOfOddEvenSums(int n) {
        long long sumOdd = (n)*n;
        long long sumEven = (n+1)*n;

        while(sumOdd>0 && sumEven>0) {
            if(sumOdd>sumEven) sumOdd = sumOdd%sumEven;
            else sumEven = sumEven%sumOdd;
        }
        if(sumOdd == 0) return sumEven;
        return sumOdd;
    }
};
