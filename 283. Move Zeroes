class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int limit=nums.size();
        vector<int>temp;
        for(auto it:nums) {
            if(it!=0) {
                temp.emplace_back(it);
            }
        }
        for(int i=0;i<temp.size();i++) {
            nums[i]=temp[i];
        }
        int temp_size=temp.size();
        for(int i=temp_size;i<limit;i++) {
            nums[i]=0;
        }
    }
};
